                },
                "count": {
                    "type": "number",
                    "example": 6
                },
                "description": {
                    "type": "string",
                    "example": "Nice Product 1"
                },
                "price": {
                    "type": "number",
                    "example": 20
                },
                "title": {
                    "type": "string",
                    "example": "Nice Product 1"
                }
            }
        }
    },
    "additionalProperties": false
}
